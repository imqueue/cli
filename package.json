{
  "name": "imq-cli",
  "version": "1.0.0-dev2",
  "description": "Command Line Interface for IMQ",
  "keywords": [
    "cli",
    "rpc-client",
    "rpc-service",
    "rpc",
    "rad",
    "message-queue",
    "redis-queue"
  ],
  "scripts": {
    "prepublish": "./node_modules/.bin/tsc",
    "test": "(./node_modules/.bin/tsc && ./node_modules/.bin/nyc mocha && ./node_modules/.bin/nyc report --reporter=text-lcov | coveralls) && npm run test-coverage",
    "test-fast": "./node_modules/.bin/tsc && ./node_modules/.bin/nyc mocha && /usr/bin/env node -e \"require('opn')('file://`pwd`/coverage/index.html', { wait: false });\"",
    "test-local": "export COVERALLS_REPO_TOKEN=$IMQ_CLI_COVERALLS_TOKEN && npm test && /usr/bin/env node -e \"require('opn')('https://coveralls.io/github/imqueue/imq-rpc', { wait: false });\"",
    "test-coverage": "cat ./coverage/lcov.info | CODECLIMATE_API_HOST=https://codebeat.co/webhooks/code_coverage CODECLIMATE_REPO_TOKEN=bafe0c12-51c6-4419-b671-cf107b5293e3 ./node_modules/.bin/codeclimate-test-reporter",
    "clean-typedefs": "find . -name '*.d.ts' -not -wholename '*node_modules*' -not -wholename '*generator*' -type f -delete",
    "clean-maps": "find . -name '*.js.map' -not -wholename '*node_modules*' -not -wholename '*generator*' -type f -delete",
    "clean-js": "find . -name '*.js' -not -wholename '*node_modules*' -not -wholename '*generator*' -type f -delete",
    "clean-tests": "rm -rf .nyc_output coverage",
    "clean-doc": "rm -rf docs",
    "clean-benchmark": "rm -rf benchmark-result",
    "clean": "npm run clean-tests && npm run clean-typedefs && npm run clean-maps && npm run clean-js && npm run clean-doc && npm run clean-benchmark",
    "doc": "rm -rf docs && typedoc --excludePrivate --excludeExternals --hideGenerator --exclude \"**/+(debug|test|node_modules|docs|coverage|benchmark|.nyc_output)/**/*\" --mode file --out ./docs . && /usr/bin/env node -e \"require('opn')('file://`pwd`/docs/index.html', { wait: false });\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/imqueue/imq-cli.git"
  },
  "bugs": {
    "url": "https://github.com/imqueue/imq-cli/issues"
  },
  "homepage": "https://imqueue.com/",
  "author": "Mykhailo Stadnyk <mikhus@gmail.com> (https://github.com/Mikhus)",
  "license": "ISC",
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/inquirer": "0.0.41",
    "chalk": "^2.4.1",
    "dotenv": "^5.0.1",
    "imq-rpc": "^1.1.6",
    "inquirer": "^5.2.0",
    "yargs": "^11.0.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "0.0.41",
    "@types/mocha": "^5.2.0",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^10.0.1",
    "@types/sinon": "^4.3.0",
    "@types/yargs": "^11.0.0",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-require": "^3.0.1",
    "nyc": "^11.4.1",
    "opn": "^5.2.0",
    "reflect-metadata": "^0.1.12",
    "sinon": "^5.0.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^6.0.2",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2"
  },
  "main": "index.js",
  "bin": {
    "imq": "./index.js"
  },
  "typescript": {
    "definitions": "index.d.ts"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/test/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary",
      "lcovonly"
    ]
  }
}
